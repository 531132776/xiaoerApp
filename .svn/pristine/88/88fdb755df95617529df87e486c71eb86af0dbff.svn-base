.common_pdd .col:nth-child(odd) {
    border-bottom: 1px solid rgba(0, 0, 0, .12);
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    border-width: 0.5px;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
}

.common_pdd .col:nth-child(even) {
    border-bottom: 1px solid rgba(0, 0, 0, .12);
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    border-width: 0.5px;
    -webkit-transform-origin: 0 0;
    transform-origin: 0 0;
}

.common_pdd .col:nth-child(odd)::before {
    position: absolute;
    bottom: 0;
    content: " ";
    right: 0;
    top: 0;
    border-right: 1px solid rgba(0, 0, 0, .12);
    -webkit-transform: scaleX(.5);
    transform: scaleX(.5);
    box-sizing: border-box;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
}

.view_cell_after::after,
.view_cell_before::before {
    position: absolute;
    bottom: 0;
    content: '';
    border-bottom: 1px solid rgba(0, 0, 0, .12);
    -webkit-transform: scaleY(0.5);
    -ms-transform: scaleY(0.5);
    -o-transform: scaleY(0.5);
    transform: scaleY(0.5);
}

.view_cell_before,
.view_cell_after {
    position: relative;
}

.view_cell_after::after,
.view_cell_before::before {
    right: 12px;
    left: 12px;
}

.view_cell-before::before {
    top: 0;
}

#app .mu-divider {
    height: 0;
    border-bottom: 1px solid rgba(0, 0, 0, .12);
